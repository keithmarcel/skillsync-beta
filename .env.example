# SkillSync Environment Variables

# Supabase
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# OpenAI (for AI skills extraction and quiz generation)
OPENAI_API_KEY=your_openai_api_key

# O*NET Web Services (for occupation data)
# Get credentials at: https://services.onetcenter.org/reference/
ONET_USERNAME=your_onet_username
ONET_PASSWORD=your_onet_password

# CareerOneStop API (for training programs and certifications)
# Get API key at: https://www.careeronestop.org/WebAPI/Home/Index
CAREERONESTP_USER_ID=your_careeronestp_user_id
CAREERONESTP_AUTH_TOKEN=your_careeronestp_auth_token

# Lightcast API (for industry skills taxonomy) - OPTIONAL
# Get credentials at: https://lightcast.io/
LIGHTCAST_CLIENT_ID=your_lightcast_client_id
LIGHTCAST_CLIENT_SECRET=your_lightcast_client_secret

# HuggingFace (optional - for LAiSER if you want to use it)
HUGGINGFACE_TOKEN=your_huggingface_token

# BLS API (for wage data) - OPTIONAL
BLS_API_KEY=your_bls_api_key
